  title = {Capacitor to {{Supercapacitor}}},
  booktitle = {Handbook of {{Nanocomposite Supercapacitor Materials I}}: {{Characteristics}}},
  author = {Banerjee, Soma and Sinha, Prerna and Verma, Kapil Dev and Pal, Tanvi and De, Bibekananda and Cherusseri, Jayesh and Manna, P. K. and Kar, Kamal K.},
