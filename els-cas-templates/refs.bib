@article{alomarGlycerolbasedDeepEutectic2016,
  title = {Glycerol-Based Deep Eutectic Solvents: {{Physical}} Properties},
  author = {Alomar, Mohamed Khalid and Hayyan, Maan and Alsaadi, Mohammed Abdulhakim and Akib, Shatirah and Hayyan, Adeeb and Hashim, Mohd Ali},
  year = 2016,
  month = mar,
  journal = {Journal of Molecular Liquids},
  volume = {215},
