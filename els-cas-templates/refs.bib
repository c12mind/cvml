@incollection{vermaCarbonNanotubeElectrode2026,
  title = {Carbon {{Nanotube}} as {{Electrode Material}} for {{Supercapacitors}}},
  booktitle = {Handbook of {{Energy Materials}} in {{Supercapacitors}} and {{Storage Devices}}},
  author = {Verma, Sanjeev and Das, Tapas and Verma, Shivani and Pandey, Vikas Kumar and Pandey, Saurabh Kumar and Singh, Juhi and Verma, Bhawna},
  year = 2026,
  pages = {161--173},
  publisher = {John Wiley \& Sons, Ltd},
  doi = {10.1002/9781119901259.ch7},
  urldate = {2026-02-11},
  abstract = {Supercapacitors with high performance hold great promise as potential future energy storage technology for electrochemical energy sources. Due to their strong mechanical characteristics, high electrochemical stability, enormous electrical conductivity, huge aspect ratio, and so on, CNTs (carbon nanotubes) are regarded as possible electrode active materials for energy storage. Furthermore, their distinctive qualities, such as novel structure, narrow size distribution, low resistivity, high surface accessible area, and high stability, both MWCNTs (multi-walled CNTs) and SWCNTs (single-walled CNTs) have been taken into consideration for electrochemical material in supercapacitor application. The EDLC mechanism is primarily responsible for the high specific capacitance value of CNTs. As a result, the physical characteristics of CNTs, like their surface area and electrical conductivity, that are associated with their fabrication and post-synthesis processes, primarily determine their supercapacitive performance. This chapter focuses typically on the most recent advancements and developments in the manufacture and post-treatment of CNTs for supercapacitor electrodes.},
  chapter = {7},
  copyright = {\copyright{} 2026 Scrivener Publishing LLC},
  isbn = {978-1-119-90125-9},
  langid = {english},
  keywords = {capacitance,carbon nanotubes,EDLC,electrolyte,energy density,Energy storage,power density,supercapacitor}
}

@article{vermaSeparationLithiumCobalt2020,
  title = {Separation of {{Lithium}} and {{Cobalt}} from {{LiCoO2}}: {{A Unique Critical Metals Recovery Process Utilizing Oxalate Chemistry}}},
  author = {Verma, Ankit and Johnson, Grant H. and Corbin, David R. and Shiflett, Mark B.},
  year = 2020,
  month = apr,
  journal = {ACS Sustainable Chemistry and Engineering},
  volume = {8},
  number = {15},
  pages = {6100--6108},
  publisher = {American Chemical Society},
  issn = {21680485},
  doi = {10.1021/ACSSUSCHEMENG.0C01128},
  urldate = {2023-03-23},
  abstract = {The demand for lithium-ion batteries (LiBs) is significantly increasing leading to a shortage in supply for critical metals, such as lithium and cobalt. Recycling LiCoO2 cathodes can provide a secondary source for these critical metals, which are necessary raw materials for the synthesis of modern LiB cathodes, such as nickel manganese cobalt oxide (NMC). In this work, an environmentally friendly, closed-loop process for recovery and separation of lithium and cobalt from LiCoO2 cathodes has been developed using oxalate chemistry. In this paper, oxalic acid and ammonium hydrogen oxalate are utilized as digestion reagents to extract lithium into the aqueous phase (Li2C2O4) and precipitate cobalt oxalate (CoC2O4{$\cdot$}2H2O) in the solid phase resulting in a low-temperature, cost-effective separation of these metals. A green-colored intermediate was identified as a Co(III)-oxalate complex; this complex further reduces and precipitates as a Co(II)-oxalate complex (CoC2O4{$\cdot$}2H2O). The optimum acidity for digestion and metals separation using oxalate containing acids was a pH {$<$} 2.5. The minimum amount of oxalic acid required for digestion was determined in order to develop the most economical process with more than 97 wt \% Li and Co recovery. The proposed process is an energy-efficient, cost-effective, environmentally friendly process for recovering high-value, critical metals, such as lithium and cobalt from LiBs and other sources.},
  keywords = {Cathode recycling,Cobalt,Critical metals,Leaching,Lithium,Oxalic acid,Spent lithium-ion batteries}
}

