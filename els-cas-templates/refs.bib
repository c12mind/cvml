@article{singhEvaluationCarbonBasedsupporting2022,
  title = {Evaluation of Carbon Based-Supporting Materials for Developing Form-Stable Organic Phase Change Materials for Thermal Energy Storage: {{A}} Review},
  shorttitle = {Evaluation of Carbon Based-Supporting Materials for Developing Form-Stable Organic Phase Change Materials for Thermal Energy Storage},
  author = {Singh, P. and Sharma, R. K. and Khalid, M. and Goyal, R. and Sar{\i}, A. and Tyagi, V. V.},
  year = 2022,
  month = oct,
  journal = {Solar Energy Materials and Solar Cells},
  volume = {246},
  pages = {111896},
  issn = {0927-0248},
  doi = {10.1016/j.solmat.2022.111896},
  urldate = {2026-02-11},
  abstract = {This paper thoroughly reviews the development and characterization of carbon-based form stable organic phase change materials (FS-OPCMs) for latent heat storage applications. The O-PCMs such as paraffin, fatty acids, polyethylene glycols (PEGs), etc., suffer from poor thermal conductivity and flow ability in their molten state, which restricts their many applications. Carbon-based materials are seen as a promising and viable way to overcome these challenges. They have a very high thermal conductivity and can hold liquid phase change materials (PCMs) in their pores. This review provides comprehensive coverage of the carbon structures and their classifications, including carbon nanotubes (CNT), carbon nanofiber (CNF), expanded graphite (EG), graphene, graphene oxide (GO), carbonized industrial solid wastes, and other carbon-based materials that can be used as supporting porous materials in developing FS-OPCMs for thermal energy storage (TES) applications. In addition, the thermal and chemical performance of carbon-based FS-OPCMs is extensively investigated and given. The applications of such composites are also discussed and summarized. Finally, the potential of these materials for thermal energy storage is presented. This review provides an in-depth insight into the potential of carbon-based materials for latent heat thermal energy storage (LHTES).},
  keywords = {Carbon based porous materials,FS-OPCMs,FSPCMs,O-PCMs,Phase change materials,Thermal energy storage}
}

