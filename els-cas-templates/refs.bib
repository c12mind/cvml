@article{kumarPretreatmentWasteBiomass2019,
  title = {Pretreatment of Waste Biomass in Deep Eutectic Solvents: {{Conductive}} Heating versus Microwave Heating},
  author = {Kumar, Narendra and Muley, Pranjali D. and Boldor, Dorin and Coty, Gary G. and Lynam, Joan G.},
