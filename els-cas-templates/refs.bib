@article{rodriguez-loyaDynamicLightScattering2023a,
  title = {Dynamic {{Light Scattering}} and {{Its Application}} to {{Control Nanoparticle Aggregation}} in {{Colloidal Systems}}: {{A Review}}},
  shorttitle = {Dynamic {{Light Scattering}} and {{Its Application}} to {{Control Nanoparticle Aggregation}} in {{Colloidal Systems}}},
  author = {{Rodriguez-Loya}, Jesus and Lerma, Maricarmen and {Gardea-Torresdey}, Jorge L.},
  year = 2023,
  month = dec,
  journal = {Micromachines},
  volume = {15},
  number = {1},
  pages = {24},
  issn = {2072-666X},
  doi = {10.3390/mi15010024},
  abstract = {Colloidal systems and their control play an essential role in daily human activities, but several drawbacks lead to an avoidance of their extensive application in some more productive areas. Some roadblocks are a lack of knowledge regarding how to influence and address colloidal forces, as well as a lack of practical devices to understand these systems. This review focuses on applying dynamic light scattering (DLS) as a powerful tool for monitoring and characterizing nanoparticle aggregation dynamics. We started by outlining the core ideas behind DLS and how it may be used to examine colloidal particle size distribution and aggregation dynamics; then, in the last section, we included the options to control aggregation in the chemically processed toner. In addition, we pinpointed knowledge gaps and difficulties that obstruct the use of DLS in real-world situations. Although widely used, DLS has limits when dealing with complicated systems, including combinations of nanoparticles, high concentrations, and non-spherical particles. We discussed these issues and offered possible solutions and the incorporation of supplementary characterization approaches. Finally, we emphasized how critical it is to close the gap between fundamental studies of nanoparticle aggregation and their translation into real-world applications, recognizing challenges in colloidal science.},
  langid = {english},
  pmcid = {PMC10819909},
  pmid = {38258143},
  keywords = {chemically processed toner,colloids,control agglomeration,theory DLVO,zeta potential}
}

